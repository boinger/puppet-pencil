# -*- mode: ruby -*-
title 'cpu usage'
area :stacked

field :cpu_system,
        :alias => "CPU/system",
        :color => 'yellow',
        :data => "stats.cpu.total.system"

field :cpu_iowait,
        :alias => "CPU/iowait",
        :color => 'red',
        :data => "stats.cpu.total.iowait"

field :cpu_nice,
        :alias => "CPU/nice",
        :color => 'green',
        :data => "stats.cpu.total.nice"

field :cpu_user,
        :alias => "CPU/user",
        :color => 'blue',
        :data => "stats.cpu.total.user"
